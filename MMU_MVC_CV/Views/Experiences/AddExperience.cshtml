@using MMU_MVC_CV.Models.Entity
@model List<TblExperiences>
@{
    ViewBag.Title = "AddExperience";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>Yeni Deneyim Ekleme Sayfası</h4>
<br />
<form class="form-group" method="post">
    <input type="text" name="Title" class="form-control" placeholder="Başlığı Giriniz..." />
    <br />
    <input type="text" name="SubTitle" class="form-control" placeholder="Alt Başlığı Giriniz..." />
    <br />
    <input type="text" name="Date" class="form-control" placeholder="Tarih Aralığı Giriniz..." />
    <br />
    <textarea name="Description" class="form-control" placeholder="Açıklama Giriniz..." rows="5"></textarea>
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>